    function items:UpdateWaterMark(vis)
        
        vis = vis or false
     
        watermark_back.Visible = vis
       
        
    end

    function items:UpdateUI(vis)
        
        vis = vis or false
     
        menu_back.Visible = vis
       
        
    end
    
    local opened = true

    UIS.InputBegan:Connect(function(input,gpe)
    
        if input.KeyCode and input.KeyCode == Enum.KeyCode.RightShift then

            if opened then
                Window:UpdateUI(false)
                opened = false
            else
                Window:UpdateUI(true)
                opened = true
            end

        end
end)

    function items:AddTab(name,image,selected)

        name = name or "New Tab"
        image = image or ""
        selected = selected or false

    local RealTab = Instance.new("ScrollingFrame")
    local Left = Instance.new("Frame")
    local Right = Instance.new("Frame")

    RealTab.Visible = false
    RealTab.Parent = tabs
    RealTab.Active = true
    RealTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    RealTab.BackgroundTransparency = 1.000
    RealTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
    RealTab.BorderSizePixel = 0
    RealTab.Position = UDim2.new(0.224999994, 0, 0.140000001, 0)
    RealTab.Size = UDim2.new(0, 578, 0, 383)
    RealTab.ScrollBarThickness = 6
    RealTab.ScrollingEnabled = true
    RealTab.Name = name

    Left.Name = "Left"
    Left.Parent = RealTab
    Left.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Left.BackgroundTransparency = 1.000
    Left.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Left.BorderSizePixel = 0
    Left.Position = UDim2.new(0, 0, -1.84061628e-05, 0)
    Left.Size = UDim2.new(0, 275, 0, 750)

    local LeftUIT = Instance.new("UIListLayout")
    LeftUIT.Parent = Left
    LeftUIT.Padding = UDim.new(0,10)

    local UIPaddingL = Instance.new("UIPadding")
    UIPaddingL.Parent = Left
    UIPaddingL.PaddingTop = UDim.new(0,5)

    Right.Name = "Right"
    Right.Parent = RealTab
    Right.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Right.BackgroundTransparency = 1.000
    Right.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Right.BorderSizePixel = 0
    Right.Position = UDim2.new(0.501730084, 0, -0.000591427495, 0)
    Right.Size = UDim2.new(0, 275, 0, 750)

    local RUIT = Instance.new("UIListLayout")
    RUIT.Parent = Right
    RUIT.Padding = UDim.new(0,10)

    local UIPaddingR = Instance.new("UIPadding")
    UIPaddingR.Parent = Right
    UIPaddingR.PaddingTop = UDim.new(0,5)

        local Tab = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")
        local ImageLabel = Instance.new("ImageLabel")

        Tab.Name = name
        Tab.Parent = tabs_buttonsFrame
        Tab.BackgroundColor3 = Color3.fromRGB(38, 36, 49)
        Tab.BackgroundTransparency = 1.000
        Tab.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Tab.BorderSizePixel = 0
        Tab.Position = UDim2.new(0.00790513866, 0, 0.256578952, 0)
        Tab.Size = UDim2.new(0, 156, 0, 40)
        Tab.Font = Enum.Font.Ubuntu
        Tab.Text = "           "..name
        Tab.TextColor3 = Color3.fromRGB(255, 255, 255)
        Tab.TextSize = 17.000
        Tab.TextStrokeTransparency = 0.850
        Tab.TextTransparency = 0.550
        Tab.TextXAlignment = Enum.TextXAlignment.Left

        UICorner.CornerRadius = UDim.new(0, 6)
        UICorner.Parent = Tab

        ImageLabel.Parent = Tab
        ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ImageLabel.BackgroundTransparency = 1.000
        ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
        ImageLabel.BorderSizePixel = 0
        ImageLabel.Position = UDim2.new(0.0402684547, 0, 0.125, 0)
        ImageLabel.Size = UDim2.new(0, 30, 0, 30)
        ImageLabel.Image = image
        ImageLabel.ImageTransparency = 0.550

        if selected then

            oldtab = Tab
            local tween; tween = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(38, 36, 49)})
            tween:Play()
            local tween2; tween2 = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {TextTransparency = 0})
            tween2:Play()
            local tween3; tween3 = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {BackgroundTransparency = 0})
            tween3:Play()
            local tween4; tween4 = game:GetService("TweenService"):Create(Tab.ImageLabel, TweenInfo.new(0.35), {ImageColor3 = Color3.fromRGB(126, 106, 226)})
            tween4:Play()
            local tween5; tween5 = game:GetService("TweenService"):Create(Tab.ImageLabel, TweenInfo.new(0.35), {ImageTransparency = 0})
            tween5:Play()

            if tabs:FindFirstChild(name) then
            tabs:FindFirstChild(name).Visible = true
            end
        end

        Tab.MouseButton1Click:Connect(function()

            if #AddonsIndeed:GetChildren() == 0 then

          
                else

                    local child = AddonsIndeed:GetChildren()[1]

                    for i,v in pairs(rift:GetDescendants()) do

                        if v:IsA("ObjectValue") then

                            child.Parent = v.Parent
                            child.Visible = false

                        end

                    end


                end

            if oldtab ~= Tab then

                local tween; tween = game:GetService("TweenService"):Create(oldtab, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(38, 36, 49)})
                tween:Play()
                local tween2; tween2 = game:GetService("TweenService"):Create(oldtab, TweenInfo.new(0.35), {TextTransparency = 0.55})
                tween2:Play()
                local tween3; tween3 = game:GetService("TweenService"):Create(oldtab, TweenInfo.new(0.35), {BackgroundTransparency = 1})
                tween3:Play()
                local tween4; tween4 = game:GetService("TweenService"):Create(oldtab.ImageLabel, TweenInfo.new(0.35), {ImageColor3 = Color3.fromRGB(255,255,255)})
                tween4:Play()
                local tween5; tween5 = game:GetService("TweenService"):Create(oldtab.ImageLabel, TweenInfo.new(0.35), {ImageTransparency = 0.55})
                tween5:Play()
                tabs:FindFirstChild(oldtab.Name).Visible = false
                task.wait()
                local tween6; tween6 = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(38, 36, 49)})
                tween6:Play()
                local tween7; tween7 = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {TextTransparency = 0})
                tween7:Play()
                local tween8; tween8 = game:GetService("TweenService"):Create(Tab, TweenInfo.new(0.35), {BackgroundTransparency = 0})
                tween8:Play()
                local tween9; tween9 = game:GetService("TweenService"):Create(Tab.ImageLabel, TweenInfo.new(0.35), {ImageColor3 = Color3.fromRGB(126, 106, 226)})
                tween9:Play()
                local tween10; tween10 = game:GetService("TweenService"):Create(Tab.ImageLabel, TweenInfo.new(0.35), {ImageTransparency = 0})
                tween10:Play()
                tabs:FindFirstChild(Tab.Name).Visible = true
                task.wait()
                oldtab = Tab
            end
        end)

