    -- Scripts:

    local function CCDX_fake_script() -- c.LocalScript 
        local script = Instance.new('LocalScript', c)

        local RS = game:GetService("RunService")
        local frames = 0
        
        RS.RenderStepped:Connect(function()
            frames = frames + 1
        end)
        
        while wait(1) do
            TextLabel_4.Text = "  FPS:" .. frames
            frames = 0
        end
    end
    coroutine.wrap(CCDX_fake_script)()

    local function SZQN_fake_script() -- d.LocalScript 
        local script = Instance.new('LocalScript', d)

        
        
        while wait(1) do
            TextLabel_5.Text = "  PING:" .. math.floor(game.Players.LocalPlayer:GetNetworkPing() * 2000)
        end
    end
    coroutine.wrap(SZQN_fake_script)()

    
    local function YKLYYCR_fake_script() -- e.LocalScript 
        local script = Instance.new('LocalScript', e)

        
        
        while task.wait(.15) do
            
            local currentTime = os.time()
            TextLabel_1.Text = "  " .. os.date("%H:%M:%S", currentTime)
        end
    end
    coroutine.wrap(YKLYYCR_fake_script)()
