                    local function VUGD_fake_script() -- ColorpickerFrame.ColorMain 
                        local script = Instance.new('LocalScript', ColorpickerFrame)

                        local player = game.Players.LocalPlayer
                        local mouse = player:GetMouse()
                        
                        local rgb = script.Parent:WaitForChild("RGB")
                        local value = script.Parent:WaitForChild("Value").ImageLabel
                        local preview = script.Parent.Parent
                        
                        local selectedColor = Color3.fromHSV(0, 0, 1)
                        local colorData = {0,0,0}
                        
                        local mouse1down = false
                        local used = false
                        
                        local function setColor(hue,sat,val,dod)
                            
                            dod = dod or false
                        
                            
                            colorData = {hue or colorData[1],sat or colorData[2],val or colorData[3]}
                            selectedColor = Color3.fromHSV(colorData[1],colorData[2],colorData[3])
                            preview.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1)
                            value.Parent.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1)
                            pcall(callback,preview.ImageColor3)
                        
                            local colorString = tostring(preview.ImageColor3)
                            local colorStringSplitted = string.split(colorString, ',')
                            
                            local first = math.round(colorStringSplitted[1] * 255)
                            local sec = math.round(colorStringSplitted[2] * 255)
                            local third = math.round(colorStringSplitted[3] * 255)
                        
                            script.Parent.G.PlaceholderText = tostring(Color3.new(first, sec, third))
                            local color = Color3.fromRGB((tonumber(colorStringSplitted[1] * 255 )), tonumber(colorStringSplitted[2] * 255), tonumber(colorStringSplitted[3] * 255))
                        
                        end
                        
                        
                        for i,v in pairs(script.Parent:GetChildren()) do
                            
                            
                            if not used then
                        
                            if v:IsA("TextBox") then
                        
                                v:GetPropertyChangedSignal("Text"):Connect(function()
                        
                                        if Color3.fromRGB(v.Text) and #v.Text ~= 0 then
                                            
                                            local colorString = v.Text
                                            local colorStringSplitted = string.split(colorString, ',')
                                            local color = Color3.fromRGB(tonumber(colorStringSplitted[1]), tonumber(colorStringSplitted[2]), tonumber(colorStringSplitted[3]))
                                            task.wait(0.1)
                                    
                                            rgb:WaitForChild("Marker").Position = UDim2.new(color.G,0,color.R,0)
                                            
                                            if tonumber(colorStringSplitted[1]) and tonumber(colorStringSplitted[2]) and tonumber(colorStringSplitted[3]) then
                                            local first = math.round(colorStringSplitted[1] * 255)
                                            local sec = math.round(colorStringSplitted[2] * 255)
                                            local third = math.round(colorStringSplitted[3] * 255)
                                            
                                            preview.ImageColor3 = Color3.fromRGB(first, sec, third)
                                            pcall(callback,preview.ImageColor3)
                                                value.Parent.ImageColor3 = Color3.fromRGB(first,sec,third)
                                            
                                            end
                                        end
                                end)
                        
                            end
                        end
                        
                        end
                        
                        
                        local function inBounds(frame)
                            local x,y = mouse.X - frame.AbsolutePosition.X,mouse.Y - frame.AbsolutePosition.Y
                            local maxX,maxY = frame.AbsoluteSize.X,frame.AbsoluteSize.Y
                            if x >= 0 and y >= 0 and x <= maxX and y <= maxY then
                               
                                if x/maxX and y/maxY then
                                    
                                end
                                return x/maxX,y/maxY
                            end
                        
                        end
                        
                        local function updateRGB()
                            if mouse1Down then
                                
                               
                                local x,y = inBounds(rgb)
                                if x and y and rgb.Parent.Visible == true then
                                  
                                  
                                    game:GetService("TweenService"):Create(rgb.Marker, TweenInfo.new(0.25), {Position = UDim2.new(x,0,y,0)}):Play()
                                    setColor(1 - x,1 - y,nil,true)
                                    
                                    used = true
                                
                              
                                end
                                
                                local x,y = inBounds(value)
                                if x and y and value.Parent.Visible == true then
                                    
                   
                                    game:GetService("TweenService"):Create(value.Parent.Marker, TweenInfo.new(0.3), {Position = UDim2.new(x,0,-1.2,0)}):Play()
                                    setColor(nil,nil,1 - y,true)
                                    preview.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1-x)
                                    pcall(callback,preview.ImageColor3)
                                    used = true
                 
                                end
                            else
                               
                               
                                used = false
                              
                            end
                        end
                        
                        mouse.Move:connect(updateRGB)
                        
                        mouse.Button1Down:connect(function()mouse1Down = true  end)
                        mouse.Button1Up:connect(function()mouse1Down = false end)
                    end
                    coroutine.wrap(VUGD_fake_script)()


                end

