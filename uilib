                Hold.BorderSizePixel = 0
                Hold.Size = UDim2.new(0, 105, 0, 35)
                Hold.BackgroundTransparency = 0
                Toggle.Parent = ActuallFrame
                Toggle.BackgroundColor3 = Color3.fromRGB(11, 10, 16)
                Toggle.BackgroundTransparency = 0.450
                Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Toggle.BorderSizePixel = 0
                Toggle.Size = UDim2.new(0, 105, 0, 35)
                pcall(callback,false)
                end)

                ToggleButton.MouseButton1Click:Connect(function()
                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                    game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                
                    toggledefault = false
                    state = "Toggle"
                    Toggle.Parent = ActuallFrame
                    Toggle.BackgroundColor3 = Color3.fromRGB(62, 56, 91)
                    Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Toggle.BorderSizePixel = 0
                    Toggle.Size = UDim2.new(0, 105, 0, 35)
                    Toggle.BackgroundTransparency = 0
                    Hold.Parent = ActuallFrame
                    Hold.BackgroundColor3 = Color3.fromRGB(11, 10, 16)
                    Hold.BackgroundTransparency = 0.450
                    Hold.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Hold.BorderSizePixel = 0
                    Hold.Size = UDim2.new(0, 105, 0, 35)
                    pcall(callback,false)
                    end)

                Toggle.Name = "Toggle"
                Toggle.Parent = ActuallFrame
                Toggle.BackgroundColor3 = Color3.fromRGB(11, 10, 16)
                Toggle.BackgroundTransparency = 0.450
                Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Toggle.BorderSizePixel = 0
                Toggle.Position = UDim2.new(0.49781552, 0, 0.379999995, 0)
                Toggle.Size = UDim2.new(0, 105, 0, 35)

                UICorner_6.Parent = Toggle

                ToggleButton.Name = "ToggleButton"
                ToggleButton.Parent = Toggle
                ToggleButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
                ToggleButton.BackgroundTransparency = 0.550
                ToggleButton.BorderColor3 = Color3.fromRGB(39, 39, 39)
                ToggleButton.BorderSizePixel = 0
                ToggleButton.Size = UDim2.new(0, 104, 0, 35)
                ToggleButton.Font = Enum.Font.Highway
                ToggleButton.Text = "Toggle"
                ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                ToggleButton.TextSize = 20.000
                ToggleButton.TextStrokeTransparency = 0.830


                UICorner_7.Parent = ToggleButton

                Toggle_2.Name = "Toggle"
                Toggle_2.Parent = ActuallFrame
                Toggle_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Toggle_2.BackgroundTransparency = 1.000
                Toggle_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Toggle_2.BorderSizePixel = 0
                Toggle_2.Position = UDim2.new(0, 0, 0.693656802, 0)
                Toggle_2.Size = UDim2.new(0, 217, 0, 34)
                Toggle_2.Font = Enum.Font.Highway
                Toggle_2.Text = "   Show In Binds"
                Toggle_2.TextColor3 = Color3.fromRGB(255, 255, 255)
                Toggle_2.TextSize = 18.000
                Toggle_2.TextStrokeTransparency = 0.770
                Toggle_2.TextTransparency = 0.690
                Toggle_2.TextXAlignment = Enum.TextXAlignment.Left

                local default2 = false
                local cooldown = false
                Toggle_2.MouseButton1Click:Connect(function()

                    if cooldown == false then
                        cooldown = true

                        if default2 == true then

                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                            game:GetService("TweenService"):Create(Toggle_2, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                            default2 = false
                          
                        else

                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0.15}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0.55, 0.5)}):Play()
                            game:GetService("TweenService"):Create(Toggle_2, TweenInfo.new(0.35), {TextTransparency = 0}):Play()
                            default2 = true
                               
                        end
                    wait(0.25)
                    cooldown = false
                    end
                end)

                SwitchButton.Name = "SwitchButton"
                SwitchButton.Parent = Toggle_2
                SwitchButton.AnchorPoint = Vector2.new(0.5, 0.5)
                SwitchButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                SwitchButton.BackgroundTransparency = 1.000
                SwitchButton.Position = UDim2.new(0.850000024, 0, 0.5, 0)
                SwitchButton.Size = UDim2.new(0.168776378, 0, 0.400000006, 0)

                Bar.Name = "Bar"
                Bar.Parent = SwitchButton
                Bar.AnchorPoint = Vector2.new(0.5, 0.5)
                Bar.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
                Bar.BackgroundTransparency = 0.300
                Bar.BorderColor3 = Color3.fromRGB(27, 42, 53)
                Bar.BorderSizePixel = 0
                Bar.Position = UDim2.new(0.596954525, 0, 0.5, 0)
                Bar.Size = UDim2.new(0.806090951, 0, 1, 0)

                UICorner_8.CornerRadius = UDim.new(1, 0)
                UICorner_8.Parent = Bar

                Circle.Name = "Circle"
                Circle.Parent = Bar
                Circle.AnchorPoint = Vector2.new(0, 0.5)
                Circle.BackgroundColor3 = Color3.fromRGB(17, 15, 25)
                Circle.BorderSizePixel = 0
                Circle.Position = UDim2.new(-0.200000003, 0, 0.5, 0)
                Circle.Size = UDim2.new(0.550000012, 0, 1.25, 0)

                UICorner_9.CornerRadius = UDim.new(1, 0)
                UICorner_9.Parent = Circle

                if state == "Hold" then
                
                    Hold.Name = "Hold"
                    Hold.Parent = ActuallFrame
                    Hold.BackgroundColor3 = Color3.fromRGB(62, 56, 91)
                    Hold.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Hold.BorderSizePixel = 0
                    Hold.Position = UDim2.new(-1.06611628e-06, 0, 0.379999995, 0)
                    Hold.Size = UDim2.new(0, 105, 0, 35)
                    Hold.BackgroundTransparency = 0

                   else
    
                    Toggle.Name = "Toggle"
                    Toggle.Parent = ActuallFrame
                    Toggle.BackgroundColor3 = Color3.fromRGB(62, 56, 91)
                    Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Toggle.BorderSizePixel = 0
                    Toggle.Position = UDim2.new(-1.06611628e-06, 0, 0.379999995, 0)
                    Toggle.Size = UDim2.new(0, 105, 0, 35)
                    Toggle.BackgroundTransparency = 0
    
                   end

                end




                return items
            end

            function items:AddDropdown(name,items,default,callback)
                
                name = name or "New Dropdown"
                items = items or {"ex1", "ex2", "ex3", "ex4"}
                skibidi = skibidi or "ex1"
                callback = callback or function() end

          

                local Dropdown = Instance.new("TextButton")
                local selected = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")
                local arrow = Instance.new("ImageLabel")
                local TextLabel = Instance.new("TextLabel")

                Dropdown.Name = name
                Dropdown.Parent = BackFrame

                local default = Instance.new("StringValue",Dropdown)
                default.Name = "default"
                default.Value = skibidi

                Dropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Dropdown.BackgroundTransparency = 1.000
                Dropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Dropdown.BorderSizePixel = 0
                Dropdown.Position = UDim2.new(0.0254545454, 0, 0.4375, 0)
                Dropdown.Size = UDim2.new(0, 275, 0, 40)
                Dropdown.Font = Enum.Font.Highway
                Dropdown.Text = "   "..name
                Dropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
                Dropdown.TextSize = 18.000
                Dropdown.TextStrokeTransparency = 0.770
                Dropdown.TextXAlignment = Enum.TextXAlignment.Left
                Dropdown.ZIndex = 1

                selected.Name = "selected"
                selected.Parent = Dropdown
                selected.BackgroundColor3 = Color3.fromRGB(56, 52, 74)
                selected.BackgroundTransparency = 0.550
                selected.BorderColor3 = Color3.fromRGB(0, 0, 0)
                selected.BorderSizePixel = 0
                selected.Position = UDim2.new(0.549908996, 0, 0.125, 0)
                selected.Size = UDim2.new(0, 115, 0, 29)
                selected.ZIndex = 1

                UICorner.Parent = selected

                arrow.Name = "arrow"
                arrow.Parent = selected
                arrow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                arrow.BackgroundTransparency = 1.000
                arrow.BorderColor3 = Color3.fromRGB(0, 0, 0)
                arrow.BorderSizePixel = 0
                arrow.Position = UDim2.new(0.800000012, 0, 0.241379306, 0)
                arrow.Rotation = 180.000
                arrow.Size = UDim2.new(0, 16, 0, 16)
                arrow.Image = "http://www.roblox.com/asset/?id=17401578232"
                arrow.ZIndex = 1

                TextLabel.Parent = selected
                TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.BackgroundTransparency = 1.000
                TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
                TextLabel.BorderSizePixel = 0
                TextLabel.Position = UDim2.new(0.00708538573, 0, 0, 0)
                TextLabel.Size = UDim2.new(0, 91, 0, 29)
                TextLabel.Font = Enum.Font.Highway
                TextLabel.Text = "  "..skibidi
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 14.000
                TextLabel.TextStrokeTransparency = 0.780
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                Dropdown.TextTransparency = 0.68

            local selecteditems = Instance.new("Frame")
            local UICorner_3 = Instance.new("UICorner")
            local UIListLayout = Instance.new("UIListLayout")
            local UIPadding = Instance.new("UIPadding")

            selecteditems.Name = "selecteditems"
            selecteditems.Parent = Dropdown
            selecteditems.BackgroundColor3 = Color3.fromRGB(20, 17, 30)
            selecteditems.BackgroundTransparency = 0
            selecteditems.BorderColor3 = Color3.fromRGB(0, 0, 0)
            selecteditems.BorderSizePixel = 0
            selecteditems.Position = UDim2.new(0.549908996, 0, 1, 0)
            selecteditems.Size = UDim2.new(0, 115, 0, 80)
            selecteditems.ZIndex = 20
            selecteditems.Visible = false
            
            UIListLayout.Parent = selecteditems
            UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            UIListLayout.Padding = UDim.new(0, 2)

            UICorner_3.Parent = selecteditems

            UIPadding.Parent = selecteditems
            UIPadding.PaddingTop = UDim.new(0, 5)

            local opened = false
            local cooldown = false

            for i,v in pairs(items) do

                local Value1 = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                Value1.Name = ""..tostring(v)
                Value1.Parent = selecteditems
                Value1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Value1.BackgroundTransparency = 1.000
                Value1.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Value1.BorderSizePixel = 0
                Value1.Size = UDim2.new(0, 115, 0, 35)
                Value1.Font = Enum.Font.Highway
                Value1.Text = "  "..tostring(v)
                Value1.TextColor3 = Color3.fromRGB(255, 255, 255)
                Value1.TextSize = 14.000
                Value1.TextTransparency = 0.680
                Value1.TextXAlignment = Enum.TextXAlignment.Left
                Value1.ZIndex = 21

                UICorner.Parent = Value1

                Value1.MouseButton1Click:Connect(function()

                    TextLabel.Text = Value1.Text

                   for i,v in pairs(selecteditems:GetChildren()) do
                 
                    if v:IsA("TextButton") then

                        if v.Text == Value1.Text then

                            local textvalue = Value1.Text:gsub(" ","")
                            default.Value = textvalue
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextTransparency = 0.14}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {BackgroundTransparency = 0.95}):Play()
                            pcall(callback,textvalue)
                        else
                            game:GetService("TweenService"):Create(v, TweenInfo.new(0.35), {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                            game:GetService("TweenService"):Create(v, TweenInfo.new(0.35), {TextTransparency = 0.680}):Play()
                            game:GetService("TweenService"):Create(v, TweenInfo.new(0.35), {BackgroundTransparency = 1}):Play()
                        end
                    end
                   end
                end)

                task.wait()

                if tostring(v) == tostring(default) then

                Value1.Name = ""..tostring(v)
                Value1.Parent = selecteditems
                Value1.BackgroundColor3 = Color3.fromRGB(255,255,255)
                Value1.BackgroundTransparency = 0.95
                Value1.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Value1.BorderSizePixel = 0
                Value1.Size = UDim2.new(0, 115, 0, 35)
                Value1.Font = Enum.Font.Highway
                Value1.Text = "  "..tostring(v)
                Value1.TextColor3 = Color3.fromRGB(158, 139, 232)
                Value1.TextSize = 14.000
                Value1.TextTransparency = 0.14
                Value1.TextXAlignment = Enum.TextXAlignment.Left

                end
            end

            Dropdown.MouseButton1Click:Connect(function()
            
                if not cooldown then
                    cooldown = true

                if not opened then

                    opened = true
                    game:GetService("TweenService"):Create(arrow, TweenInfo.new(0.35), {Rotation = 0}):Play()
                    game:GetService("TweenService"):Create(selecteditems, TweenInfo.new(0.35), {Size = UDim2.new(0,115,0,selecteditems.UIListLayout.AbsoluteContentSize.Y + 8)}):Play()
                    selecteditems.Visible = true
                else

                    opened = false
                    game:GetService("TweenService"):Create(arrow, TweenInfo.new(0.35), {Rotation = 180}):Play()
                    game:GetService("TweenService"):Create(selecteditems, TweenInfo.new(0.35), {Size = UDim2.new(0,115,0,0)}):Play()
                    task.wait(0.15)
                    selecteditems.Visible = false

                end

                wait(0.25)
                cooldown = false
            end

            end)

            end

            function items:AddMultiDropdown(name,items,skibidi,callback)
                
                name = name or "New Dropdown"
                items = items or {"ex1", "ex2", "ex3", "ex4"}
                skibidi = skibidi or {}
                callback = callback or function() end
          

                local Dropdown = Instance.new("TextButton")
                local selected = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")
                local arrow = Instance.new("ImageLabel")
                local TextLabel = Instance.new("TextLabel")

                Dropdown.Name = name
                Dropdown.Parent = BackFrame

                local default = Instance.new("StringValue",Dropdown)
                default.Name = "default"
                default.Value = tostring(table.concat(skibidi, ", "))

                Dropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Dropdown.BackgroundTransparency = 1.000
                Dropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Dropdown.BorderSizePixel = 0
                Dropdown.Position = UDim2.new(0.0254545454, 0, 0.4375, 0)
                Dropdown.Size = UDim2.new(0, 275, 0, 40)
                Dropdown.Font = Enum.Font.Highway
                Dropdown.Text = "   "..name
                Dropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
                Dropdown.TextSize = 18.000
                Dropdown.TextStrokeTransparency = 0.770
                Dropdown.TextXAlignment = Enum.TextXAlignment.Left
                Dropdown.ZIndex = -1
                
                selected.Name = "selected"
                selected.Parent = Dropdown
                selected.BackgroundColor3 = Color3.fromRGB(56, 52, 74)
                selected.BackgroundTransparency = 0.550
                selected.BorderColor3 = Color3.fromRGB(0, 0, 0)
                selected.BorderSizePixel = 0
                selected.Position = UDim2.new(0.549908996, 0, 0.125, 0)
                selected.Size = UDim2.new(0, 115, 0, 29)
                selected.ZIndex = -1

                UICorner.Parent = selected

                arrow.Name = "arrow"
                arrow.Parent = selected
                arrow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                arrow.BackgroundTransparency = 1.000
                arrow.BorderColor3 = Color3.fromRGB(0, 0, 0)
                arrow.BorderSizePixel = 0
                arrow.Position = UDim2.new(0.800000012, 0, 0.241379306, 0)
                arrow.Rotation = 180.000
                arrow.Size = UDim2.new(0, 16, 0, 16)
                arrow.Image = "http://www.roblox.com/asset/?id=17401578232"
                arrow.ZIndex = -1

                TextLabel.Parent = selected
                TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.BackgroundTransparency = 1.000
                TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
                TextLabel.BorderSizePixel = 0
                TextLabel.Position = UDim2.new(0.00708538573, 0, 0, 0)
                TextLabel.Size = UDim2.new(0, 91, 0, 29)
                TextLabel.Font = Enum.Font.Highway
                if #skibidi > 1 then
                TextLabel.Text = "  "..skibidi[1]..", ..."
                elseif #skibidi == 0 then
                    TextLabel.Text = "  ..."
                else
                TextLabel.Text = "  "..skibidi[1]
                end
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 14.000
                TextLabel.TextStrokeTransparency = 0.780
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                Dropdown.TextTransparency = 0.68

            local selecteditems = Instance.new("Frame")
            local UICorner_3 = Instance.new("UICorner")
            local UIListLayout = Instance.new("UIListLayout")
            local UIPadding = Instance.new("UIPadding")

            selecteditems.Name = "selecteditems"
            selecteditems.Parent = Dropdown
            selecteditems.BackgroundColor3 = Color3.fromRGB(20, 17, 30)
            selecteditems.BackgroundTransparency = 0
            selecteditems.BorderColor3 = Color3.fromRGB(0, 0, 0)
            selecteditems.BorderSizePixel = 0
            selecteditems.Position = UDim2.new(0.549908996, 0, 1, 0)
            selecteditems.Size = UDim2.new(0, 115, 0, 80)
            selecteditems.ZIndex = 20
            selecteditems.Visible = false

            UIListLayout.Parent = selecteditems
            UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            UIListLayout.Padding = UDim.new(0, 2)

            UICorner_3.Parent = selecteditems

            UIPadding.Parent = selecteditems
            UIPadding.PaddingTop = UDim.new(0, 5)

            local opened = false
            local cooldown = false

            for i,v in pairs(items) do

                local Value1 = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")

                Value1.Name = ""..tostring(v)
                Value1.Parent = selecteditems
                Value1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Value1.BackgroundTransparency = 1.000
                Value1.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Value1.BorderSizePixel = 0
                Value1.Size = UDim2.new(0, 115, 0, 35)
                Value1.Font = Enum.Font.Highway
                Value1.Text = "  "..tostring(v)
                Value1.TextColor3 = Color3.fromRGB(255, 255, 255)
                Value1.TextSize = 14.000
                Value1.TextTransparency = 0.680
                Value1.TextXAlignment = Enum.TextXAlignment.Left
                Value1.ZIndex = 21

                UICorner.Parent = Value1

                Value1.MouseButton1Click:Connect(function()

                    TextLabel.Text = Value1.Text

                   for i,v in pairs(selecteditems:GetChildren()) do
                 
                    if v:IsA("TextButton") then

                        local textt = Value1.Text:gsub(" ","")

                        if Value1.TextColor3 == Color3.fromRGB(255, 255, 255) then
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextTransparency = 0.14}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {BackgroundTransparency = 0.95}):Play()
                            
                            if not table.find(skibidi, textt) then
                            table.insert(skibidi,textt)
                        
                            default.Value = tostring(table.concat(skibidi, ", "))
                            pcall(callback,{default.Value})

                            if #skibidi > 1 then
                                TextLabel.Text = "  "..skibidi[1]..", ..."
                                elseif #skibidi == 0 then
                                    TextLabel.Text = "  ..."
                                else
                                TextLabel.Text = "  "..skibidi[1]
                                end

                      

                            end
                        else
                          
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {TextTransparency = 0.680}):Play()
                            game:GetService("TweenService"):Create(Value1, TweenInfo.new(0.35), {BackgroundTransparency = 1}):Play()
                            if table.find(skibidi, textt) then
                            local index = table.find(skibidi, textt)
                            table.remove(skibidi,index)
                        
                            default.Value = tostring(table.concat(skibidi, ", "))
                            pcall(callback,{default.Value})
                            if #skibidi > 1 then
                                TextLabel.Text = "  "..skibidi[1]..", ..."
                                elseif #skibidi == 0 then
                                    TextLabel.Text = "  ..."
                                else
                                TextLabel.Text = "  "..skibidi[1]
                                end

                          

                            task.wait(0.05)
                        end
                        end
                    end
                   end
                end)

                task.wait()

                if table.find(skibidi,v) then

                Value1.Name = ""..tostring(v)
                Value1.Parent = selecteditems
                Value1.BackgroundColor3 = Color3.fromRGB(255,255,255)
                Value1.BackgroundTransparency = 0.95
                Value1.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Value1.BorderSizePixel = 0
                Value1.Size = UDim2.new(0, 115, 0, 35)
                Value1.Font = Enum.Font.Highway
                Value1.Text = "  "..tostring(v)
                Value1.TextColor3 = Color3.fromRGB(158, 139, 232)
                Value1.TextSize = 14.000
                Value1.TextTransparency = 0.14
                Value1.TextXAlignment = Enum.TextXAlignment.Left

                end
            end

            Dropdown.MouseButton1Click:Connect(function()
            
                if not cooldown then
                    cooldown = true

                if not opened then

                    opened = true
                    game:GetService("TweenService"):Create(arrow, TweenInfo.new(0.35), {Rotation = 0}):Play()
                    game:GetService("TweenService"):Create(selecteditems, TweenInfo.new(0.35), {Size = UDim2.new(0,115,0,selecteditems.UIListLayout.AbsoluteContentSize.Y + 8)}):Play()
                    selecteditems.Visible = true
                else

                    opened = false
                    game:GetService("TweenService"):Create(arrow, TweenInfo.new(0.35), {Rotation = 180}):Play()
                    game:GetService("TweenService"):Create(selecteditems, TweenInfo.new(0.35), {Size = UDim2.new(0,115,0,0)}):Play()
                    task.wait(0.15)
                    selecteditems.Visible = false

                end

                wait(0.25)
                cooldown = false
            end

            end)

            end


            function items:AddSlider(name,max,default,suffix,callback)

                name = name or "New Slider"
                max = max or 200
                default = default or 100
                addby = addby or 1
                suffix = suffix or ""
                callback = callback or function() end

                local Slider = Instance.new("Frame")
                local SilderIndeed = Instance.new("Frame")
                local Point = Instance.new("ImageLabel")
                local MouseOn = Instance.new("ImageLabel")
                local MouseDown = Instance.new("ImageLabel")
