    function items:Notify(text,typee,duration)

        text = text or "Besic Notification Text"
        typee = typee or "Success"
        duration = duration or 4

        if typee == "Success" then

        local SuccessNotification = Instance.new("Frame")
        local UIPadding = Instance.new("UIPadding")
        local Slidee = Instance.new("Frame")
        local icon = Instance.new("ImageLabel")
        local Status = Instance.new("TextLabel")
        local Textt = Instance.new("TextLabel")

        --Properties:

        SuccessNotification.Name = "SuccessNotification"
        SuccessNotification.Parent = NotificationsFrame
        SuccessNotification.BackgroundColor3 = Color3.fromRGB(9, 8, 13)
        SuccessNotification.BackgroundTransparency = 0.050
        SuccessNotification.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SuccessNotification.BorderSizePixel = 0
        SuccessNotification.Position = UDim2.new(0.345574856, 0, 0.34720844, 0)
        SuccessNotification.Size = UDim2.new(0, 520, 0, 58)

        UIPadding.Parent = SuccessNotification
        UIPadding.PaddingLeft = UDim.new(0, 7)
        UIPadding.PaddingTop = UDim.new(0, 7)

        Slidee.Name = "Slidee"
        Slidee.Parent = SuccessNotification
        Slidee.BackgroundColor3 = Color3.fromRGB(43, 175, 36)
        Slidee.BackgroundTransparency = 0.800
        Slidee.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Slidee.BorderSizePixel = 0
        Slidee.Position = UDim2.new(-0.0136452243, 0, -0.137254298, 0)
        Slidee.Size = UDim2.new(0, 0, 0, 58)

        icon.Name = "icon"
        icon.Parent = SuccessNotification
        icon.BackgroundColor3 = Color3.fromRGB(64, 40, 85)
        icon.BackgroundTransparency = 1.000
        icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
        icon.BorderSizePixel = 0
        icon.Position = UDim2.new(0, 0, 0.169720218, 0)
        icon.Size = UDim2.new(0, 25, 0, 25)
        icon.ZIndex = 3
        icon.Image = "http://www.roblox.com/asset/?id=17416489656"
        icon.ImageColor3 = Color3.fromRGB(43, 175, 36)

        Status.Name = "Status"
        Status.Parent = SuccessNotification
        Status.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Status.BackgroundTransparency = 1.000
        Status.BorderColor3 = Color3.fromRGB(0, 0, 0)
