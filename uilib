            function items:AddToggle(name,skibidi,callback)

                name = name or "New Toggle"
                skibidi = skibidi or false
                callback = callback or function() end
                toggledefault = default or false

                local OGToggle = Instance.new("TextButton")
                local SwitchButton = Instance.new("Frame")
                local Bar = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")
                local Circle = Instance.new("Frame")
                local UICorner_2 = Instance.new("UICorner")
                local Button = Instance.new("ImageButton")

                OGToggle.Name = name
                OGToggle.Parent = BackFrame

                local default = Instance.new("BoolValue",OGToggle)
                default.Name = "default"
                default.Value = skibidi

                OGToggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                OGToggle.BackgroundTransparency = 1.000
                OGToggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                OGToggle.BorderSizePixel = 0
                OGToggle.Position = UDim2.new(0.0254545454, 0, 0.4375, 0)
                OGToggle.Size = UDim2.new(0, 275, 0, 40)
                OGToggle.Font = Enum.Font.Highway
                OGToggle.Text = "   "..name
                OGToggle.TextColor3 = Color3.fromRGB(255, 255, 255)
                OGToggle.TextSize = 18.000
                OGToggle.TextStrokeTransparency = 0.770
                OGToggle.TextXAlignment = Enum.TextXAlignment.Left
                OGToggle.ZIndex = -1
                OGToggle.TextTransparency = 0.68

                SwitchButton.Name = "SwitchButton"
                SwitchButton.Parent = OGToggle
                SwitchButton.AnchorPoint = Vector2.new(0.5, 0.5)
                SwitchButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                SwitchButton.BackgroundTransparency = 1.000
                SwitchButton.Position = UDim2.new(0.850000024, 0, 0.5, 0)
                SwitchButton.Size = UDim2.new(0.168776378, 0, 0.400000006, 0)
                SwitchButton.ZIndex = -1

                Bar.Name = "Bar"
                Bar.Parent = SwitchButton
                Bar.AnchorPoint = Vector2.new(0.5, 0.5)
                Bar.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
                Bar.BackgroundTransparency = 0.550
                Bar.BorderColor3 = Color3.fromRGB(27, 42, 53)
                Bar.BorderSizePixel = 0
                Bar.Position = UDim2.new(0.596954525, 0, 0.5, 0)
                Bar.Size = UDim2.new(0.806090951, 0, 1, 0)
                Bar.ZIndex = -1

                UICorner.CornerRadius = UDim.new(1, 0)
                UICorner.Parent = Bar

                Circle.Name = "Circle"
                Circle.Parent = Bar
                Circle.AnchorPoint = Vector2.new(0, 0.5)
                Circle.BackgroundColor3 = Color3.fromRGB(17, 15, 25)
                Circle.BorderSizePixel = 0
                Circle.Position = UDim2.new(0, 0, 0.5, 0)
                Circle.Size = UDim2.new(0.550000012, 0, 1.25, 0)
                Circle.BackgroundTransparency = 0.15
                Circle.ZIndex = -1

                UICorner_2.CornerRadius = UDim.new(1, 0)
                UICorner_2.Parent = Circle
    

                --

                local cooldown = false
                OGToggle.MouseButton1Click:Connect(function()

                    if cooldown == false then
                        cooldown = true

                        if default.Value == true then

                            
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                            game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                            default.Value = false
                            toggledefault = false
                            pcall(callback,default.Value)
                          
                        else
                  
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0.15}):Play()
                            game:GetService("TweenService"):Create(Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0.55, 0.5)}):Play()
                            game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0}):Play()
                            default.Value = true
                            toggledefault = true
                            pcall(callback,default.Value)
                        end
                    wait(0.25)
                    cooldown = false
                    end
                end)

