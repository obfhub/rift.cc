                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0.55, 0.5)}):Play()
                                game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0}):Play()
                                pcall(callback,true)
                            else

                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                                game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                                toggledefault = false
                                pcall(callback,false)

                            end
                        end
                    elseif state == "Hold" then
                                  
                        Circle2 = OGToggle.SwitchButton.Bar.Circle

                        if input.KeyCode == bind or input.UserInputType == bind then

                                if UIS:IsKeyDown(bind) then
                            if not toggledefault then

                                toggledefault = true
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0.15}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0.55, 0.5)}):Play()
                                game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0}):Play()
                                pcall(callback,true)

                            else

                                toggledefault = false
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                                game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                                pcall(callback,false)
                            end
                        end
                        end

                      end
                      waiting = false
                    end)
                
                    UIS.InputEnded:Connect(function(input, GPE) 
                    
                        
                        state = state
                        bind = bind
                        
                        if state == "Hold" then
                                  
                            Circle2 = OGToggle.SwitchButton.Bar.Circle

                            if input.KeyCode == bind or input.UserInputType == bind then
    
                                if toggledefault then
                                toggledefault = false
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(17, 15, 25)}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0}):Play()
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0, 0.5)}):Play()
                                game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0.68}):Play()
                                pcall(callback,false)
                                else
                                    toggledefault = true
                                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(158, 139, 232)}):Play()
                                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundTransparency = 0.15}):Play()
                                    game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {Position = UDim2.fromScale(0.55, 0.5)}):Play()
                                    game:GetService("TweenService"):Create(OGToggle, TweenInfo.new(0.35), {TextTransparency = 0}):Play()
                                    pcall(callback,true)
                                end
                            end
    
                          end

                    end)
                    
                end)


                UICorner_3.Parent = TextBox

