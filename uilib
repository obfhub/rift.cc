                function InputToString(Input)
                    if Input then
                        local String = (tostring(Input) .. "."):gsub("%.", ",")
                        local Count = 0
                        --
                        for Value in String:gmatch("(.-),") do
                            Count = Count + 1
                            --
                            if Count == 3 then
                                String = Value:gsub("Keypad", "")
                            end
                        end
                        --
                        if String == "Unknown" or Input.Value == 27 then
                          
                            return "   None"
                        elseif InputNames[String] then
                           
                            String = InputNames[String]
                            
                        end
                        --
                       
                        return String
                    else
                        
                        return "   None"
                    end
                end

                local inp = UIS.InputBegan
                
                TextBox.MouseButton1Click:Connect(function()
                
                    pcall(callback,false)
                    toggledefault = false

                    state = state 
                    waiting = true
                    TextBox.Text = "   '...'"
                    

                    inp:Connect(function(input, GPE) 


                        state = state
                        bind = bind
    
                        if waiting then
                        if input.KeyCode ~= Enum.KeyCode.Unknown then
    
                            bind = input.KeyCode
    
                            waiting = false
                            local string = InputToString(input.KeyCode)
                            
                             TextBox.Text = "   '"..string.."'"
                            
                        else
                                bind = input.UserInputType
                            
                                waiting = false
                                local string = InputToString(input.UserInputType)
                                TextBox.Text = "   '"..string.."'"
                              
                        end
                    end

                    if state == "Toggle" then
                
                        Circle2 = OGToggle.SwitchButton.Bar.Circle

                        if input.KeyCode == bind or input.UserInputType == bind then

                            if not toggledefault then

                                toggledefault = true
                                game:GetService("TweenService"):Create(OGToggle.SwitchButton.Bar.Circle, TweenInfo.new(0.35), {BackgroundColor3 = Color3.fromRGB(158, 139, 232)}):Play()
