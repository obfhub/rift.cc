                Grid.Name = "Grid"
                Grid.Value = false

                local GridColor3 = Instance.new("Color3Value",SilderIndeed)
                GridColor3.Name = "GridColor3"
                GridColor3.Value = Color3.fromRGB(51, 45, 74)
                
